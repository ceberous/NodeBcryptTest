extends base

block vars
	- var title = 'Dashboard'

block body
	h1 Dashboard
	p.
		Welcome to your Dashboard! You are Now Logged In!

	h2 Current User Information
	p First Name: #{user.firstName}	
	p Last Name: #{user.lastName}
	p Email: #{user.email}
	p Data: #{user.data}
	p Password: #{user.password}